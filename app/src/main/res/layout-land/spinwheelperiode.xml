<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scaleType="fitXY"
    android:background="@drawable/white_background"
    tools:context=".Mainspinwheel_periode">
    <RelativeLayout
        android:id="@+id/group_385"
        android:layout_width="190dp"
        android:layout_height="31dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/vector"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:background="@drawable/gp3_arrow"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/scan_tutup_"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="3sp"
            android:text="EkstraVaganza"
            android:textAppearance="@style/redeemvagan" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rel_text"
        android:layout_width="413dp"
        android:layout_height="151dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.034"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group_385">

        <TextView
            android:id="@+id/sistem"
            android:layout_width="46dp"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="66dp"
            android:layout_toStartOf="@+id/pengguna_me"
            android:lineSpacingExtra="9sp"
            android:text="@string/sistem"
            android:textAppearance="@style/sistem" />

        <TextView
            android:id="@+id/pengguna_me"
            android:layout_width="220dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="52dp"
            android:gravity="center_vertical"
            android:text="@string/pengguna_me"
            android:textAppearance="@style/pengguna_me" />

        <TextView
            android:id="@+id/penukaran"
            android:layout_width="73dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sistem"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="41dp"
            android:layout_marginEnd="53dp"
            android:layout_toStartOf="@+id/setiap_akhi"
            android:gravity="center_vertical"
            android:text="@string/penukaran"
            android:textAppearance="@style/penukaran" />

        <TextView
            android:id="@+id/setiap_akhi"
            android:layout_width="223dp"
            android:layout_height="16dp"
            android:layout_below="@+id/pengguna_me"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="47dp"
            android:gravity="center_vertical"
            android:text="@string/setiap_akhir"
            android:textAppearance="@style/setiap_akhi" />

        <View
            android:id="@+id/line_12"
            android:layout_width="385dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="128dp"
            android:background="@drawable/line_12" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="315dp"
        android:layout_height="46dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/group_915"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rel_text" />

    <ImageView
        android:id="@+id/wheel"
        android:layout_width="290dp"
        android:layout_height="280dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/wheel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group_385" />


    <ImageView
        android:id="@+id/wheel_bg"
        android:layout_width="290dp"
        android:layout_height="280dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/arrow_wheel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/group_385" />

    <Button
        android:id="@+id/rectangle_11"
        android:layout_width="307dp"
        android:layout_height="53dp"
        android:layout_marginTop="268dp"
        android:background="@drawable/rectangle_11"
        android:text="Redeem"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>